---
import BookmarkItem from 'src/components/BookmarkItem.astro'
import type { Bookmark } from 'src/types/raindrop'
import HomeRightSection from 'src/components/HomeRightSection.astro'

const {bookmarks} = Astro.props as {
	bookmarks: Bookmark[]
}
---

<HomeRightSection header="Latest Bookmarks">
	{bookmarks.map((bookmark) => (
		<BookmarkItem
			bookmark={bookmark}
		/>
	))}
</HomeRightSection>
